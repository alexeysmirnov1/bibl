{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": ["laravel", "framework"],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "ext-exif": "*",
        "ext-simplexml": "*",
        "ext-xmlreader": "*",
        "darkaonline/l5-swagger": "^8.6",
        "ebess/advanced-nova-media-library": "^4.4",
        "flagstudio/da-data-address-yandex-map": "dev-main",
        "gamez/typed-collection": "^7.1",
        "guzzlehttp/guzzle": "^7.9",
        "http-interop/http-factory-guzzle": "^1.2",
        "laravel/framework": "^10.48",
        "laravel/horizon": "^5.33",
        "laravel/nova": "4.28.0",
        "laravel/sanctum": "^3.3",
        "laravel/scout": "^10.17",
        "laravel/tinker": "^2.10",
        "livewire/livewire": "^3.6",
        "meilisearch/meilisearch-php": "^1.13",
        "naoray/nova-json": "^3.1",
        "predis/predis": "^2.4",
        "spatie/laravel-data": "^4.13",
        "spatie/laravel-medialibrary": "^10.15",
        "spatie/laravel-view-models": "^1.6",
        "think.studio/nova-flexible-content": "^4.2",
        "whitecube/nova-flexible-content": "^1"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.15",
        "fakerphp/faker": "^1.24.1",
        "imanghafoori/laravel-microscope": "^1.0",
        "larastan/larastan": "^2.0",
        "laravel/pint": "^1.20",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^7.12",
        "pestphp/pest": "^2.36",
        "phpunit/phpunit": "^10.1",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Literati\\": "src/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        },
        "files": [
            "tests/utilities/functions.php"
        ]
    },
    "scripts": {
        "pint": "vendor/bin/pint --config=./pint-base.json --test -v",
        "pintfix": "vendor/bin/pint --config=./pint-base.json -v",
        "larastan": "./vendor/bin/phpstan analyse"
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        },
        {
            "type": "path",
            "url": "./nova-components/DaDataAddressYandexMap"
        }
    ]
}
